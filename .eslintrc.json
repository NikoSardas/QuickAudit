{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-console": "off"
  },
  "globals": {
    "window": "readonly",
    "document": "readonly",
    "navigator": "readonly"
  }
}
